{
  "name": "test-package-release",
  "version": "1.0.0",
  "private": true,
  "description": "test repository for package release",
  "license": "UNLICENSED",
  "author": "",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write . ",
    "prepare": "husky install && husky set .husky/prepare-commit-msg 'exec < /dev/tty && npx git cz --hook || true' && husky set .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:KonishiKenji/test-package-release.git"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.1.0",
    "commitizen": "^4.2.4",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "semantic-release": "19.0.1",
    "@semantic-release/npm": "9.0.0",
    "semantic-release-monorepo": "7.0.5"
  },
  "config": {
    "commitizen": {
      "path": "@digitalroute/cz-conventional-changelog-for-jira"
    }
  }
}
